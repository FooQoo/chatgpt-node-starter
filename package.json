{
  "name": "chatgpt-node-starter",
  "version": "1.0.0",
  "description": "chatgpt nodejs project for hackathon",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec node --loader ts-node/esm src/index.ts",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "start:dev": "cross-env NODE_ENV=development node ./dist/api/index.js",
    "start": "node ./dist/api/index.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,ts,json}'",
    "prepare": "husky install"
  },
  "repository": "git@github.com:FooQoo/chatgpt-node-starter.git",
  "author": "FooQoo <horseshoes.buzz.17@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "body-parser": "^1.20.2",
    "chatgpt": "^5.2.2",
    "express": "^4.18.2",
    "log4js": "^6.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/log4js": "^2.3.5",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "2.8.7",
    "rimraf": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  }
}
